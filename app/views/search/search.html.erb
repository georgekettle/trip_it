<div class="search-page-container">
  <div class="search-page-results">
    <h3><%= @posts.count %> results found</h3>
    <!-- results go here -->
    <%= will_paginate @posts %>
    <% @posts.each do |post| %>
      <%= render 'posts/post', post: post %>
    <% end %>
  </div>
  <div id="map" class="side-map-sticky" data-controller="map" data-mapbox-token="<%= ENV['MAPBOX_TOKEN'] %>" data-center="<%= @lng_lat %>" data-posts="<%= @posts.to_json(include: [:photo, :location]) %>" data-features="<%= @features.to_json %>"></div>
</div>
